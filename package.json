{
  "name": "battlecode-playback",
  "version": "2017.0.0",
  "description": "Play back and analyze battlecode match files.",
  "author": "Teh Devs",
  "license": "GPL-3.0",
  "private": true,
  "main": "out/src/index.js",
  "typings": "out/src/index.d.ts",
  "scripts": {
    "clean": "rm -rf out",
    "build": "tsc -p .",
    "lint": "tslint \"**/*.ts\" -e \"out/**\"",
    "test": "npm run build && blue-tape \"out/test/**/*.js\" | tap-dot",
    "watch": "onchange -w \"**/*.ts\" -i -e \"out/**\" -- npm -s run build+test",
    "publish": "npm -s run lint+build+test && echo please publish to typings/registry",
    "lint+build+test": "npm run lint && npm run build+test"
  },
  "repository": {
    "type": "git",
    "url": "github.com/battlecode/battlecode-playback"
  },
  "dependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.43",
    "@types/victor": "^0.2.28",
    "battlecode-schema": "git+ssh://git@github.com/battlecode/battlecode-schema.git#c6e2f68c04059e91f77a87e074e9a3b8e5e27066",
    "core-js": "^2.4.0",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.30",
    "blue-tape": "^0.2.0",
    "onchange": "^2.5.0",
    "stream": "0.0.2",
    "tap-dot": "^1.0.5",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "typings": "^1.3.3"
  }
}
